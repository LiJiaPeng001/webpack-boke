<div aria-label="Page navigation">
  <ul class="pagination">
  {{#pageArray}}
  {{#disabled}}
    <li class='disabled'><span data-value={{value}}>{{name}}</span></li>
    {{/disabled}}
    {{^disabled}}
    {{#active}}
        <li class='active'><span data-value={{value}}>{{name}}</span></li>
    {{/active}}
    {{#active}}
        <li><span data-value={{value}}>{{name}}</span></li>
    {{/active}}
    {{/disabled}}
  {{/pageArray}}
  </ul>
</div>